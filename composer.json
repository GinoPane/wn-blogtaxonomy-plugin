{
  "name": "ginopane/wn-blogtaxonomy-plugin",
  "type": "winter-plugin",
  "description": "A robust Winter CMS plugin which extends Winter Blog Plugin with useful taxonomies",
  "homepage": "https://github.com/GinoPane/wn-blogtaxonomy-plugin",
  "keywords": ["wintercms", "cms", "blog", "taxonomy", "gino pane"],
  "authors": [
    {
      "name": "Siarhei <Gino Pane> Karavai",
      "homepage": "http://sergey.karavay.name",
      "email": "sergey.karavay@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.9",
    "winter/wn-blog-plugin": "^2.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^0.12.85",
    "winter/storm": "^1.1",
    "phpmd/phpmd": "^2.10",
    "winter/wn-system-module": "^1.1",
    "winter/wn-cms-module": "^1.1",
    "winter/wn-backend-module": "^1.1"
  },
  "support": {
    "issues": "https://github.com/GinoPane/wn-blogtaxonomy-plugin/issues",
    "discord": "https://discord.gg/D5MFSPH6Ux",
    "source": "https://github.com/GinoPane/wn-blogtaxonomy-plugin"
  },
  "scripts": {
    "psr2check": [
      "@php vendor/bin/phpcs --standard=PSR2 src/"
    ],
    "psr2autofix": [
      "@php vendor/bin/phpcbf --standard=PSR2 src/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
